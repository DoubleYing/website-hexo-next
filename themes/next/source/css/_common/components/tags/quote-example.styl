// quote with box with title: example
.quote-example {
    position: relative;
    padding: 10px;
    border: 1px solid #ddd;
    margin-bottom: 18px;
    font-size: 12px;

    &::before {
    display: inline-block;
    margin-bottom: 12px;
    margin-right: 5px;
    font-weight: 700;
    color: #959595;
    text-transform: uppercase;
    letter-spacing: 1px;
    content: "Example - ";
  }
}